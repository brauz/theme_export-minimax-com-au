</section><section class="featuredBlog article">
    <div class="container">        {% assign current_found = false %}
        {% assign done = false %}
        {% for a in blog.articles %}
        	{% if current_found and done == false %}
        		{% assign next_article = a %}
        		{% assign done = true %}
        	{% endif %}
        	{% unless done %}
        		{% if a.id == article.id %}
        			{% assign current_found = true %}
        		{% else %}
        			{% assign prev_article = a %}
        		{% endif %}
        	{% endunless %}
        {% endfor %}        {% if prev_article %}
        <div class="blogWrapper previous">
            <div class="half image" style="background-image: url( {{ prev_article.image | img_url: 'master' }} );">
            </div>
            <div class="half text">
                <div class="v-wrap">
                    <div class="v-box">
                        <div class="contentWrapper">
                            {% for tag in prev_article.tags %}
                                <h6 style="display: inline-block;">{{ tag }}{% unless forloop.last %}, {% endunless %}</h6>
                            {% endfor %}
                            <h2>
                                <a href="{{ prev_article.url }}">{{ prev_article.title }}</a>
                            </h2>                            <a href="{{ prev_article.url }}" class="std link prev">
                                Previous
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        {% endif %}        {% if next_article %}
        <div class="blogWrapper previous">
            <div class="half image" style="background-image: url( {{ next_article.image | img_url: 'master' }} );">
            </div>
            <div class="half text">
                <div class="v-wrap">
                    <div class="v-box">
                        <div class="contentWrapper">
                            {% for tag in next_article.tags %}
                                <h6 style="display: inline-block;">{{ tag }}{% unless forloop.last %}, {% endunless %}</h6>
                            {% endfor %}
                            <h2>
                                <a href="{{ next_article.url }}">{{ next_article.title }}</a>
                            </h2>                            <a href="{{ next_article.url }}" class="std link">
                                Next
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        {% endif %}    </div></section>{% include 'atb' zone:"Bottom"%}
